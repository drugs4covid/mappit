<#ACM_SIGKDD>
         rr:logicalTable [ rr:tableName "papers"];
         rr:subjectMap [
                 rr:template "http://cmt#ACM_SIGKDD/{id}";
                 rr:class sigkdd:ACM_SIGKDD
         ];
         rr:predicateObjectMap [
                 rr:predicate sigkdd:design;
                 rr:objectMap [
                         rr:parentTriplesMap <#Deadline>;
                         rr:joinCondition [
                                 rr:child "decision";
                                 rr:parent "id";
                         ];
                 ];
         ];
         rr:predicateObjectMap [
                 rr:predicate sigkdd:hold;
                 rr:objectMap [
                         rr:parentTriplesMap <#Conference>;
                         rr:joinCondition [
                                 rr:child "paper_id";
                                 rr:parent "id";
                         ];
                 ];
         ].

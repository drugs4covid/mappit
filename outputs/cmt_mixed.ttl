<#Acceptance>
         rr:logicalTable [ rr:tableName "acceptances"];
         rr:subjectMap [
                 rr:template "http://cmt#Acceptance/{id}";
                 rr:class cmt:Acceptance
         ].
<#Administrator>
         rr:logicalTable [ rr:tableName "administrators"];
         rr:subjectMap [
                 rr:template "http://cmt#Administrator/{id}";
                 rr:class cmt:Administrator
         ];
         rr:predicateObjectMap [
                 rr:predicate cmt:email;
                 rr:objectMap [ rr:column "id"];
         ];
         rr:predicateObjectMap [
                 rr:predicate cmt:acceptPaper;
                 rr:objectMap [
                         rr:parentTriplesMap <#Paper>;
                         rr:joinCondition [

<#Webmaster>
         rr:logicalTable [ rr:tableName "persons"];
         rr:subjectMap [
                 rr:template "http://sigkdd#Webmaster/{id}";
                 rr:class sigkdd:Webmaster
         ];
         rr:predicateObjectMap [
                 rr:predicate sigkdd:E-mail;
                 rr:objectMap [ rr:column "email"];
         ];
         rr:predicateObjectMap [
                 rr:predicate sigkdd:Name;
                 rr:objectMap [ rr:column "name"];
         ];
         rr:predicateObjectMap [
                 rr:predicate sigkdd:Nation;
                 rr:objectMap [ rr:column "nation"];
         ];
         rr:predicateObjectMap [
                 rr:predicate sigkdd:pay;
                 rr:objectMap [
                         rr:parentTriplesMap <#Registration_fee>;
                         rr:joinCondition [
                                 rr:child "type";
                                 rr:parent "id";
                         ];
                 ];
         ].
